([(([{{[({<[[<(){}>[[]<>]]]<[{()<>}<(){}>]>>[{((<>[]))}]}(<[[{()()}{{}<>}][(()[]>{{}{}}]][<({}[])>["""

example_data = """\
[({(<(())[]>[[{[]{<()<>>
[(()[<>])]({[<{<<[]>>(
{([(<{}[<>[]}>{[]{[(<()>
(((({<>}<{<{<>}{[]{[]{}
[[<[([]))<([[{}[[()]]]
[{[{({}]{}}([{[{{{}}([]
{<[[]]>}<{[{[{[]{()[[[]
[<(<(<(<{}))><([]([]()
<{([([[(<>()){}]>(<<{{
<{([{{}}[<[[[<>{}]]]>[]]"""


opening = dict(["()", "{}", "[]", "<>"])

charpoints = {
    ")": 3, # points.
    "]": 57, # points.
    "}": 1197, # points.
    ">": 25137, # points.
}


def find_errors(doc):
    """
    >>> find_errors("[({(<(())[]>[[{[]{<()<>>")
    []
    >>> find_errors("{([(<{}[<>[]}>{[]{[(<()>")
    [(0, '}')]
    >>> sum(charpoints[c] for ln, c in find_errors(example_data))
    26397
    """
    fails = list()
    for i, line in enumerate(doc.split("\n")):
        matches = list()
        for char in line:
            if char in opening:
                matches.append(opening[char])
            else:
                if matches[-1] != char:
                    fails.append((i, char))
                    break
                else:
                    matches.pop()
    return fails


import doctest
doctest.testmod(optionflags=doctest.REPORT_UDIFF|doctest.NORMALIZE_WHITESPACE|doctest.ELLIPSIS)


print(sum(charpoints[c] for ln, c in find_errors(actual_data)))
