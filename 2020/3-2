forest = doc.split("\n")

l = len(forest)
w = len(forest[0])

countsprod = 1
for xinc, yinc in ((1, 1), (3, 1), (5, 1), (7, 1), (1, 2)):
    count = 0
    pos = 0, 0

    while pos[1] < len(forest):
        pos = pos[0]+xinc, pos[1]+yinc

        if pos[1] > len(forest)-1:
            print(xinc, yinc, "hit", count, "trees")
            countsprod *= count
            break

        if forest[pos[1]][pos[0]%w] != ".":
            count += 1

print(countsprod)
