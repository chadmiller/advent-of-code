from functools import lru_cache

def attach(starting_at, remaining_adapters):
    target = max(remaining_adapters)

    @lru_cache(maxsize=None)
    def _attach(starting_at):
        if starting_at == target:
            return 1
        return sum(_attach(starting_at + n) for n in range(1, 4) if starting_at + n in remaining_adapters)

    return _attach(starting_at)


assert attach(0, set(map(int, ex1doc.split()))) == 8
assert attach(0, set(map(int, ex2doc.split()))) == 19208

print(attach(0, set(map(int, doc.split()))))
