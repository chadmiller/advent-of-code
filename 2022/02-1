example_data = """\
A Y
B X
C Z"""

def wintieloss(l, r):
    """
    >>> wintieloss("A", "X")
    3
    >>> wintieloss("A", "Y")
    6
    >>> wintieloss("A", "Z")
    0
    >>> wintieloss("B", "X")
    0
    >>> wintieloss("B", "Y")
    3
    >>> wintieloss("B", "Z")
    6
    >>> wintieloss("C", "Z")
    3
    >>> wintieloss("C", "Y")
    0
    >>> wintieloss("C", "X")
    6
    """
    l = (ord(l) - ord("A"))
    r = (ord(r) - ord("X"))
    r_wins_over = ((r - 1) + 3) % 3
    if l == r:
        return 3
    if r_wins_over == l:
        return 6
    return 0


def round(l, r):
    """
    >>> round("A", "Y")
    8
    >>> round("B", "X")
    1
    >>> round("C", "Z")
    6
    """
    shape_value = ord(r) - ord("X") + 1
    return wintieloss(l, r) + shape_value


def solve(raw_data):
    """
    >>> solve(example_data)
    15
    """
    return sum(round(line[0], line[2]) for line in raw_data.split("\n"))


import doctest
doctest.testmod(optionflags=doctest.REPORT_UDIFF|doctest.NORMALIZE_WHITESPACE|doctest.ELLIPSIS)

print(solve(actual_data))
