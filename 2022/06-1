#!/usr/bin/python3

import doctest
from aoc_lib import *

actual_data = """"""


def solve(raw_data):
    """
    >>> solve("mjqjpqmgbljsphdztnvjfqwrcgsmlb")
    7
    >>> solve("bvwbjplbgvbhsrlpgdmjqwftvncz")
    5
    >>> solve("nppdvjthqldpwncqszvftbrmjlhg")
    6
    >>> solve("nznrnfrfntjfmvfwmzdfjlvtqnbhcprsg")
    10
    >>> solve("zcfzfwzzqfrljwzlrfnpqdbhtmscgvjw")
    11
    """
    for i in range(4, len(raw_data)+1):
        window = raw_data[i-4:i]
        if len(set(window)) == 4:
            return i


import doctest
doctest.testmod(optionflags=doctest.REPORT_UDIFF|doctest.NORMALIZE_WHITESPACE|doctest.ELLIPSIS)

print(solve(actual_data))
